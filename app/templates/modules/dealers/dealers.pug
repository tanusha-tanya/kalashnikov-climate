mixin dealers(text)
  .dealers
    .dealers__search
      form(action="").dealers__filter
        input(type="hidden", name="filter[TYPE_SALES]", value="all")
        input(type="hidden", name="filter[COUNTRY]", value="")
        input(type="hidden", name="filter[CITY]", value="")
        input(type="hidden", name="bx_ajax_id", value="3cdac6678741927492717099cb424f94")
        fieldset.dealers__fieldset
          span Страна
          label.dealers__label
            input.dealers__input(type="text", placeholder="Страна", data-for ="filter[COUNTRY]", data-list = [{'ID':'8','NAME':'Россия','PROPERTY_ID':'24'}])
            svg.dealers__svg(role="img")(width="14")(height="20"): use(xlink:href="#baloon")
            .autocompleet
        fieldset.dealers__fieldset
          span Город
          label.dealers__label
            input.dealers__input.dealers__input-city(type="text", placeholder="Город", data-for = "filter[CITY]", data-list ='[{"ID":"16","NAME":"\u041c\u043e\u0441\u043a\u0432\u0430","PROPERTY_ID":"31", "XY":[{"X":"55.794572022089","Y":"37.654877291396"},{"X":"55.83266592946","Y":"37.686528723589"}, {"X":"55.750876618464","Y":"37.583049004507"}]}, {"ID":"17","NAME":"\u0421\u0430\u043d\u043a\u0442 \u043f\u0435\u0442\u0435\u0440\u0431\u0443\u0440\u0433","PROPERTY_ID":"31","XY":[{"X":"59.936174", "Y":"30.311934"},{"X":"59.939095", "Y":"30.315868"}, {"X":"56.461621","Y":"53.803678"}]}]')
